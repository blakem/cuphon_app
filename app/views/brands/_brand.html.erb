<tr>
  <td class="brand">
	<b><%= brand.title %></b>
  </td>
  <td>
    <%= brand.merchant ? brand.merchant.name : "None" %>
  </td>
  <td>
	<%=raw brand.brand_aliases.map { |a| a.alias }.join('<br/>') %>
  </td>
  <td>
	<%= time_ago_in_words(brand.created_at).gsub(/^about\s+/, '') %> ago
  </td>
  <td>
	<%= brand.subscriptions.count %>
  </td>
  <td>
	<%= brand.flags %>
  </td>
</tr>